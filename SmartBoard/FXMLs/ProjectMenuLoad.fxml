<VBox fx:id="menuPane" alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="400.0" spacing="15.0" styleClass="menupane" stylesheets="@../css/overlay.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="overlays.ProjectMenuLoad">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="titletext" text="Select Project to Load" />
      <ListView fx:id="projectList" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: #2C8C99;" />
      <HBox alignment="CENTER" prefHeight="31.0" prefWidth="598.0" spacing="30.0">
         <children>
            <Button fx:id="selectProject" mnemonicParsing="false" onAction="#selectProject" text="Select Project" />
            <Button fx:id="goBack" mnemonicParsing="false" text="Go Back" />
         </children>
      </HBox>
   </children>
</VBox>
